{
  "manifest_version": 3,
  "name": "Webnotes",
  "description": "Create helpful notes and reminders for your favorite websites with Webnotes!",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "tabs", "activeTab", "notifications", "storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "48": "48.png",
    "128": "128.png"
  }
}
