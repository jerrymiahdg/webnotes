{
  "manifest_version": 3,
  "name": "My Google Extension",
  "description": "description",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "tabs", "activeTab", "notifications", "storage"],
  "host_permissions": ["<all_urls>"]
}
